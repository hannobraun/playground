main:
    @position call @vec2_read call
        @set_pixel call

    @submit call

    @main jump


# Display

set_pixel:
    @x_y_to_index call
    0xffffffff
        write

    return

x_y_to_index:
    32 * +
    return

submit:
    yield
    return


# Game State

position:
    1032
    return


# Utilities

vec2_read:
    0 copy read        # x
    1 copy 2 drop read # y
    return
